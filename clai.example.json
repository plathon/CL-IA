    "explain": [
        {
            "folder": "./src/bin",
            "explanation": "contains the source code of the commands"
        }
    ],
    "commands": [
        {
            "name": "example",
            "args": ["name"],
            "templates": [{
                "template": "{{name}}-controller.ts",
                "outFile": "./src/controllers/{{name}}-controller.ts"
