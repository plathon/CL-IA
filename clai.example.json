            {
                "template": "{{name}}-view.ts",
                "outFile": "./src/views/{{name}}-view.ts"
            }],
            "instructions": ["you should import the state lib from react package inside every page"]
        }
    ],
    "extensions": [".ts"],
    "includes": ["./src/bin", "./src/schemas"]
}